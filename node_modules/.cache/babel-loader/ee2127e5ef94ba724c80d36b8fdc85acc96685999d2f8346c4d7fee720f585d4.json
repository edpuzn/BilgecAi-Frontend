<section class=\"image-gallery\">\n      <div class=\"image-item\" v-for=\"(image, index) in images\" :key=\"index\">\n        <div class=\"image-wrapper\">\n          <img :src=\"image.src\" :alt=\"image.alt\" />\n        </div>\n        <div class=\"text-content\">\n          <p class=\"caption\">{{ image.caption }}</p>\n          <p class=\"description\">{{ image.description }}</p>\n        </div>\n      </div>\n    </section>\n  </div>\n</template>\n\n<script>\nimport Bilgecan from '@/assets/anasayfa/mat-teach.png';\nimport Fizik from '@/assets/anasayfa/fizik-teach.png';\nimport Kimya from '@/assets/anasayfa/kimya-teach.png';\nexport default {\n  name: 'AnaSayfa',\n  data() {\n    return {\n      images: [\n        {\n          src: Bilgecan,\n          alt: 'Matematik Dersi',\n          caption: 'Matematiğin Sınırsız Dünyasına Hoş Geldiniz',\n          description: 'Matematik dersinde, analitik düşünceyi geliştiren ve gerçek hayat problemlerine çözümler üreten konuları keşfedeceksiniz.'\n        },\n        {\n          src: Fizik,\n          alt: 'Fizik Dersi',\n          caption: 'Fizik Kuralları ile Evreni Anlama',\n          description: 'Fizik dersinde, evrenin işleyişini anlamanıza yardımcı olacak kuvvet, hareket ve enerji gibi temel konularla tanışacaksınız.'\n        },\n        {\n          src: Kimya,\n          alt: 'Kimya Dersi',\n          caption: 'Kimyanın Gizemli Reaksiyonları',\n          description: 'Kimya dersinde, maddelerin yapısını ve bu yapıların nasıl dönüştüğünü keşfederek kimyanın büyülü dünyasına adım atın.'\n        }\n      ]\n    }\n  }\n}\n</script>\n\n<style scoped>\n.anasayfa {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  background: linear-gradient(135deg, #f5f7fa, #c3cfe2);\n  min-height: 100vh;\n  font-family: 'Poppins', sans-serif;\n  color: #333;\n  padding: 20px;\n}\n\n.lorem-section {\n  text-align: center;\n  margin-bottom: 40px;\n  padding: 20px;\n  background: rgba(255, 255, 255, 0.8);\n  border-radius: 15px;\n  box-shadow: 0 10px 15px rgba(0, 0, 0, 0.1);\n  max-width: 800px;\n}\n\n.lorem-section h1 {\n  color: #4256b9;\n  font-size: 2.5em;\n  margin-bottom: 20px;\n}\n\n.lorem-section p {\n  font-size: 1.2em;\n  line-height: 1.6;\n  color: #555;\n}\n\n.bilgecan-section {\n  margin-bottom: 20px;\n  display: flex;\n  justify-content: center;\n}\n\n.bilgecan-logo {\n  max-width: 200px; /* Resmin genişliğini belirledik */\n  height: auto;\n  border-radius: 50%;\n  box-shadow: 0 10px 20px rgba(0, 0, 0, 0.15);\n  transition: transform 0.3s ease, box-shadow 0.3s ease;\n}\n\n.bilgecan-logo:hover {\n  transform: scale(1.1);\n  box-shadow: 0 15px 30px rgba(0, 0, 0, 0.4);\n  transition: transform 0.5s ease, box-shadow 0.5s ease;\n}\n\n.image-gallery {\n  display: grid;\n  grid-template-columns: repeat(3, 1fr);\n  gap: 30px;\n  max-width: 1200px;\n  width: 100%;\n  margin: 0 auto;\n  padding: 20px;\n}\n\n.image-item {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  background: rgba(255, 255, 255, 0.9);\n  padding: 20px;\n  border-radius: 15px;\n  box-shadow: 0 10px 20px rgba(0, 0, 0, 0.15);\n  transition: transform 0.4s ease, box-shadow 0.4s ease;\n}\n\n.image-wrapper {\n  width: 100%;\n  max-width: 350px;\n  border-radius: 10px;\n  margin-bottom: 15px;\n  overflow: hidden;\n  box-shadow: 0 5px 15px rgba(0, 0, 0, 0.2);\n}\n\n.image-item:hover {\n  transform: translateY(-10px) scale(1.05);\n  box-shadow: 0 15px 30px rgba(0, 0, 0, 0.3);\n}\n\n.image-item img {\n  width: 100%;\n  height: auto;\n  object-fit: cover;\n  transition: transform 0.4s ease;\n}\n\n.image-item:hover img {\n  transform: scale(1.05);\n}\n\n.text-content {\n  text-align: center;\n}\n\n.caption {\n  font-size: 1.3em;\n  font-weight: 600;\n  color: #333;\n  margin-top: 10px;\n}\n\n.description {\n  font-size: 1.1em;\n  line-height: 1.6;\n  color: #555;\n  margin-top: 10px;\n}\n\n@media (max-width: 768px) {\n  .image-gallery {\n    grid-template-columns: 1fr;\n  }\n  .lorem-section {\n    padding: 15px;\n  }\n  .lorem-section h1 {\n    font-size: 2em;\n  }\n}\n\nbody {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n  overflow-x: hidden;\n}\n</style>\n"],"mappings":";OAIWA,UAA2B;;EAH/BC,KAAK,EAAC;AAAU;;EAeVA,KAAK,EAAC;AAAe;;EAErBA,KAAK,EAAC;AAAe;mBAlBlC;;EAqBaA,KAAK,EAAC;AAAc;;EACpBA,KAAK,EAAC;AAAS;;EACfA,KAAK,EAAC;AAAa;;uBAtB9BC,mBAAA,CA0BM,OA1BNC,UA0BM,GAzBJC,mBAAA,oBAAuB,E,0BACvBC,mBAAA,CAEU;IAFDJ,KAAK,EAAC;EAAkB,IAC/BI,mBAAA,CAAwE;IAAnEC,GAA2B,EAA3BN,UAA2B;IAACO,GAAG,EAAC,UAAU;IAACN,KAAK,EAAC;2BAGxDG,mBAAA,uBAA0B,E,0BAC1BC,mBAAA,CAMU;IANDJ,KAAK,EAAC;EAAe,IAC5BI,mBAAA,CAAuC,aAAnCA,mBAAA,CAA8B,gBAAtB,eAAa,E,GACzBA,mBAAA,CAEE,WAFC,ohBAEL,E,sBAIAA,mBAAA,CAUU,WAVVG,UAUU,I,kBATRN,mBAAA,CAQMO,SAAA,QAzBZC,WAAA,CAiBuDC,KAAA,CAAAC,MAAM,EAjB7D,CAiBsCC,KAAK,EAAEC,KAAK;yBAA5CZ,mBAAA,CAQM;MARDD,KAAK,EAAC,YAAY;MAAmCc,GAAG,EAAED;QAC7DT,mBAAA,CAEM,OAFNW,UAEM,GADJX,mBAAA,CAAyC;MAAnCC,GAAG,EAAEO,KAAK,CAACP,GAAG;MAAGC,GAAG,EAAEM,KAAK,CAACN;4BAnB5CU,UAAA,E,GAqBQZ,mBAAA,CAGM,OAHNa,UAGM,GAFJb,mBAAA,CAA0C,KAA1Cc,UAA0C,EAAAC,gBAAA,CAApBP,KAAK,CAACQ,OAAO,kBACnChB,mBAAA,CAAkD,KAAlDiB,UAAkD,EAAAF,gBAAA,CAAxBP,KAAK,CAACU,WAAW,iB","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}