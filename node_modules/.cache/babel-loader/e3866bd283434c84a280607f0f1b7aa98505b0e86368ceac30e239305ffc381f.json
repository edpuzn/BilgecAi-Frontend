ard\" v-for=\"(step, index) in steps\" :key=\"index\">\r\n          <div class=\"step-number\">{{ index + 1 }}</div>\r\n          <div class=\"step-icon\">\r\n            <i :class=\"step.icon\"></i>\r\n          </div>\r\n          <h3>{{ step.title }}</h3>\r\n          <p>{{ step.description }}</p>\r\n        </div>\r\n      </section>\r\n  \r\n      <!-- FAQ Bölümü -->\r\n      <section class=\"faq-section\">\r\n        <h2>Sıkça Sorulan Sorular</h2>\r\n        <div class=\"faq-item\" v-for=\"(faq, index) in faqs\" :key=\"index\">\r\n          <div class=\"faq-question\" @click=\"toggleFaq(index)\">\r\n            <h3>{{ faq.question }}</h3>\r\n            <i class=\"fas\" :class=\"faq.open ? 'fa-chevron-up' : 'fa-chevron-down'\"></i>\r\n          </div>\r\n          <div v-if=\"faq.open\" class=\"faq-answer\">\r\n            <p>{{ faq.answer }}</p>\r\n          </div>\r\n        </div>\r\n      </section>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    name: 'NasilKullanilir',\r\n    data() {\r\n      return {\r\n        steps: [\r\n          { title: 'Üye Olun', icon: 'fas fa-user-plus', description: 'BilgecAI\\'ye kolayca kayıt olun.' },\r\n          { title: 'İlk Sorunuzu Sorun', icon: 'fas fa-question-circle', description: 'Sohbet paneline girin ve merak ettiklerinizi yazın.' },\r\n          { title: 'BilgecAI Cevaplasın', icon: 'fas fa-comments', description: 'BilgecAI’den en iyi cevapları alın ve öğrenmeye devam edin.' }\r\n        ],\r\n        faqs: [\r\n          { question: 'BilgecAI nasıl çalışıyor?', answer: 'BilgecAI, yapay zeka ile sorularınızı analiz eder ve uygun cevapları sunar.', open: false },\r\n          { question: 'Nasıl üye olabilirim?', answer: 'Sayfanın sağ üst köşesindeki kayıt butonunu kullanarak üye olabilirsiniz.', open: false }\r\n        ]\r\n      };\r\n    },\r\n    methods: {\r\n      toggleFaq(index) {\r\n        this.faqs[index].open = !this.faqs[index].open;\r\n      }\r\n    }\r\n  };\r\n  </