{"ast":null,"code":"import SearchComponent from './components/SearchComponent.vue';\nexport default {\n  name: 'App',\n  components: {\n    SearchComponent\n  }\n};","map":{"version":3,"names":["SearchComponent","name","components"],"sources":["/Users/ed1p_/Desktop/HackAthon/frontend/src/App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <header class=\"app-header\">\n      <h1>Gemini AI Arama Uygulaması</h1>\n    </header>\n    <div class=\"app-body\">\n      <SearchComponent />\n    </div>\n  </div>\n</template>\n\n<script>\nimport SearchComponent from './components/SearchComponent.vue';\n\nexport default {\n  name: 'App',\n  components: {\n    SearchComponent,\n  },\n};\n</script>\n\n<style scoped>\n/* Uygulama genel düzenleme ve görsel şölen */\n#app {\n  font-family: 'Avenir', Helvetica, Arial, sans-serif;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 20px;\n  background: linear-gradient(to right, #ffecd2, #fcb69f);\n  min-height: 100vh;\n  display: flex;\n  flex-direction: column;\n  justify-content: flex-start;\n  align-items: center;\n}\n\n.app-header {\n  background-color: #ff6f61;\n  color: white;\n  padding: 20px;\n  width: 100%;\n  text-align: center;\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);\n}\n\n.app-body {\n  width: 100%;\n  display: flex;\n  justify-content: center;\n}\n\n\n.chat-container {\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  width: 80%;\n  max-width: 800px;\n  margin: 20px auto;\n  background-color: #f9f9f9;\n  border-radius: 15px;\n  box-shadow: 0 10px 15px rgba(0, 0, 0, 0.1);\n  overflow: hidden;\n  border: 1px solid #ddd;\n}\n\n/* Başlık stili */\n.header {\n  background-color: #4a90e2;\n  color: white;\n  text-align: center;\n  padding: 15px;\n  font-size: 1.8rem;\n}\n\n/* Sohbet geçmişi alanı */\n.chat-box {\n  height: 400px;\n  overflow-y: auto;\n  padding: 20px;\n  background-color: #fff;\n}\n\n/* Mesaj stili */\n.message {\n  margin-bottom: 15px;\n  display: flex;\n  flex-direction: column;\n  padding: 10px;\n  border-radius: 10px;\n  max-width: 70%;\n  word-wrap: break-word;\n  box-shadow: 0 5px 10px rgba(0, 0, 0, 0.1);\n}\n\n/* Kullanıcı ve AI mesajları için farklı renkler */\n.user {\n  align-self: flex-end;\n  background-color: #e0f7fa;\n  color: #00796b;\n  animation: slide-in 0.5s ease-out;\n}\n\n.ai {\n  align-self: flex-start;\n  background-color: #e3f2fd;\n  color: #0d47a1;\n  animation: slide-in 0.5s ease-out;\n}\n\n/* Mesaj içeriği */\n.message-content {\n  font-size: 1rem;\n}\n\n/* Bold ve italik yazıları destekleme */\n.message-content strong {\n  font-weight: bold;\n}\n\n.message-content em {\n  font-style: italic;\n}\n\n/* Giriş alanı */\n.input-area {\n  display: flex;\n  align-items: center;\n  padding: 15px;\n  background-color: #fafafa;\n  border-top: 1px solid #ddd;\n}\n\ninput {\n  flex: 1;\n  padding: 15px;\n  border-radius: 30px;\n  border: 1px solid #ccc;\n  font-size: 1rem;\n  margin-right: 10px;\n  box-shadow: 0 3px 6px rgba(0, 0, 0, 0.1);\n}\n\n.search-button {\n  background-color: #ff6f61;\n  color: white;\n  border: none;\n  padding: 15px 25px;\n  border-radius: 30px;\n  cursor: pointer;\n  font-size: 1rem;\n  transition: background-color 0.3s ease, transform 0.3s ease;\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);\n}\n\n.search-button:hover {\n  background-color: #e55a4f;\n  transform: translateY(-2px);\n}\n\n.search-button:active {\n  transform: translateY(2px);\n}\n\n@keyframes slide-in {\n  0% {\n    transform: translateY(20px);\n    opacity: 0;\n  }\n  100% {\n    transform: translateY(0);\n    opacity: 1;\n  }\n}\n</style>\n"],"mappings":"AAYA,OAAOA,eAAc,MAAO,kCAAkC;AAE9D,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}